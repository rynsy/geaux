## Test 18
 Checks for proper working of input/output statements
  (used by all test programs)

 Assume input is: -7, -14, 22
##

package p18geaux
func main() {
    var x int;
    var y int;
    var z int;
    print "Testing Program p18geaux\n";
    read x, y, z;
    if ((x != ~7) || (y != ~14)) || (z != 22) {
        print "\nERROR: Read error\n";
    }
    x = 1000;
    y = 2000;
    z = 3000;
    print "Testing print:  Output is ", x, z, y, "\n";
    print "Test completed\n";
} // func main
